{
  "kind": "collectionType",
  "collectionName": "asset_categories",
  "info": {
    "singularName": "asset-category",
    "pluralName": "asset-categories",
    "displayName": "Asset Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "asset_subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::asset-subcategory.asset-subcategory",
      "mappedBy": "category"
    },
    "assets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::asset.asset",
      "mappedBy": "category"
    }
  }
}
